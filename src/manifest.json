{
    "app_id": "com.mattermost.zendesk",
    "app_type": "aws_lambda",
    "version": "0.2.1",
    "display_name": "Zendesk",
    "description": "Zendesk cloud app for Mattermost",
    "homepage_url": "https://github.com/mattermost/mattermost-app-zendesk",
    "root_url": "",
    "icon": "zendesk-profile-icon.png",
    "requested_permissions": [
        "act_as_user",
        "act_as_bot",
        "remote_webhooks",
        "remote_oauth2"
    ],
    "requested_locations": [
        "/command",
        "/channel_header",
        "/post_menu",
        "/in_post"
    ],
    "aws_lambda": [
        {
            "path": "/",
            "name": "js-function",
            "handler": "index.handler",
            "runtime": "nodejs14.x"
        }
    ],
    "bindings": {
        "path": "/bindings",
        "expand": {
            "acting_user": "all",
            "channel": "summary",
            "oauth2_app": "all",
            "oauth2_user": "all"
        }
    },
    "assets": []
}
